{
  "tasks": {
    "start": "deno --allow-net --allow-read --allow-env --env-file=.env main.ts"
  },
  "imports": {
    "@octokit/rest": "npm:@octokit/rest@^21",
    "@std/assert": "jsr:@std/assert@^1"
  },
  "fmt": {
    "indentWidth": 2,
    "lineWidth": 10000,
    "proseWrap": "preserve",
    "semiColons": true,
    "singleQuote": true,
    "useTabs": false
  },
  "lock": false
}
